{
  "name": "jeraby-two",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "ts-merge-config": "tsc mergeConfigs.ts --module nodenext --target ESNext",
    "gen-config": "pnpm ts-merge-config && node mergeConfigs.js",
    "dev": "pnpm gen-config && vite --port 3333 --open",
    "build": "pnpm gen-config && vite-ssg build",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy-functions": "firebase deploy --only functions",
    "deploy-all": "firebase deploy --only hosting,functions",
    "lint": "eslint .",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unhead/vue": "^1.8.8",
    "@vueuse/core": "^10.6.1",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.3.4",
    "i18next": "^23.7.11",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.10",
    "vue-demi": "^0.14.6",
    "vue-router": "^4.2.5",
    "vue3-carousel": "^0.3.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.3.1",
    "@iconify-json/carbon": "^1.1.24",
    "@types/node": "^18.15.5",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue-macros/volar": "^0.17.4",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.14",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-format": "^0.0.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.11.0",
    "postcss": "^8.4.21",
    "rollup": "^4.6.1",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.2.7",
    "taze": "^0.13.0",
    "typescript": "^5.3.2",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.5",
    "vite-bundle-visualizer": "^0.11.0",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-pwa": "^0.17.2",
    "vite-plugin-vue-devtools": "^1.0.0-rc.6",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vite-ssg": "^0.23.5",
    "vite-ssg-sitemap": "^0.6.1",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.24"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
